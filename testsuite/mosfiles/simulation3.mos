val(x,0.5);
val(u,0.5);
val(u2,0.5);
val(y,0.5);
system("rm -f InputOutput_*");
system("rm -f InputOutput.cpp InputOutput.exe InputOutput.makefile InputOutput.log InputOutput.libs");


loadFile("InitTest.mo");
