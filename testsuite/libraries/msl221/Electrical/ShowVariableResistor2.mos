"R2.v:";
val(R2.v,0.0);
val(R2.v,0.25);
val(R2.v,0.5);
val(R2.v,0.75);
val(R2.v,1.0);

system("rm Modelica.Electrical.Analog.Examples.ShowVariableResistor*");


