loadFile("dyn2_pkg.mo");
loadFile("dyn2.mo");
simulate(Dyn2, startTime=0.0, stopTime=1.0, numberOfIntervals=2, tolerance=1e-5);
echo(false);
size := readSimulationResultSize("Dyn2_res.plt");
res:=readSimulationResult("Dyn2_res.plt",{x},size);
x := res[1];
x := 1000*x;
x := floor(x);
echo(true);
x/1000.0;
readFile("output.log");
