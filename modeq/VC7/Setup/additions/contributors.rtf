\par 
\par >> list(Modelica.Electrical.Analog.Basic.Resistor)
\par "model Resistor "Ideal linear electrical resistor"
\par   extends Interfaces.OnePort;
\par   parameter SI.Resistance R=1 "Resistance";
\par   annotation(Documentation(info="<HTML>
\par <P>
\par The linear resistor connects the branch voltage <i>v</i> with the
\par branch current <i>i</i> by <i>i*R = v</i>.
\par The Resistance <i>R</i> is allowed to be positive, zero, or negative.
\par </P>
\par </HTML>
\par "),Coordsys(extent=[-100,-100;100,100],grid=[2,2],component=[20,20]),
\par Icon(Rectangle(extent=[-70,30;70,-30],style(color=3,fillColor=7,
\par fillPattern=1)),
\par Line(points=[-90,0;-70,0]),Line(points=[70,0;90,0]),
\par Text(extent=[-98,-58;102,-120],string="R=%R",style(color=0)),
\par Text(extent=[-100,40;100,100],string="%name")),
\par Diagram(Rectangle(extent=[-70,30;70,-30]),
\par Line(points=[-90,0;-70,0]),Line(points=[70,0;90,0])),
\par Window(x=0.2,y=0.06,width=0.62,height=0.69));
\par 
\par equation 
\par   R*i=v;
\par end Resistor;
\par 
\par "
\par 
\par >> instantiateModel(Modelica.Electrical.Analog.Basic.Resistor)
\par "fclass Modelica.Electrical.Analog.Basic.Resistor
\par Real v;
\par Real i;
\par Real p.v;
\par Real p.i;
\par Real n.v;
\par Real n.i;
\par parameter Real R = 1;
\par equation
